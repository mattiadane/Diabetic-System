<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<VBox alignment="CENTER" spacing="20.0"

      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.dashapp.diabeticsystem.controllers.LoginController" >


    <Label text="username" alignment="TOP_LEFT"/>
    <TextField fx:id="usernameField"/>
    <Label  text="password" alignment="TOP_LEFT"/>
    <PasswordField fx:id="passwordField"/>
    <Button  text="Login" onAction="#onClickLogin" styleClass="loginButton"/>
</VBox>
