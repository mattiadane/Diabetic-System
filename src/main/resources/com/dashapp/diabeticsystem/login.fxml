<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.dashapp.diabeticsystem.controllers.LoginController" >
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="usernameLabel" text="username" alignment="TOP_LEFT"/>
    <TextField fx:id="usernameField"/>
    <Label fx:id="passwordLabel" text="password" alignment="TOP_LEFT"/>
    <PasswordField fx:id="passwordField"/>
    <Button fx:id="confirmButton" text="Login" onAction="#onClickLogin" styleClass="loginButton"/>
</VBox>
